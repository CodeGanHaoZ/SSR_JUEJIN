{
  "kind": "collectionType",
  "collectionName": "article_infos",
  "info": {
    "singularName": "article-info",
    "pluralName": "article-infos",
    "displayName": "articleInfo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "aid": {
      "type": "biginteger",
      "required": true,
      "unique": true,
      "max": "999999",
      "min": "100000"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "classification_tab": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::classification-tab.classification-tab"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "text",
      "maxLength": 80,
      "required": true,
      "unique": true
    },
    "introduction": {
      "type": "text",
      "maxLength": 100,
      "minLength": 50,
      "required": true
    },
    "type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::type.type"
    },
    "content": {
      "type": "richtext",
      "required": true
    }
  }
}
